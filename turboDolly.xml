						<innerRim offset="0.04" />
                    </wheel>
                </wheels>
				<objectChange node="wheelLeftHub" translationActive="0.045 0 0" />
				<objectChange node="wheelRightHub" translationActive="-0.045 0 0" />
            </wheelConfiguration>
        </wheelConfigurations>

        <rimColor material="0">SHARED_GREY2</rimColor>

        <hubs>
            <color0 useBaseColor="true" />
            <color1>SHARED_GREY5</color1>
            <hub linkNode="wheelLeftHub"  filename="$data/shared/wheels/hubs/hub_n10_5.xml" isLeft="true"  scale="0.333 0.333 0.333" />
            <hub linkNode="wheelRightHub" filename="$data/shared/wheels/hubs/hub_n10_5.xml" isLeft="false" scale="0.333 0.333 0.333" />
        </hubs>
    </wheels>

    <dynamicallyLoadedParts>
        <dynamicallyLoadedPart node="0" linkNode="wheelChockSupportLeft" filename="$data/shared/assets/wheelChocks/wheelChockSupport01.i3d"/>
        <dynamicallyLoadedPart node="0" linkNode="wheelChockSupportRight" filename="$data/shared/assets/wheelChocks/wheelChockSupport01.i3d"/>
		<!-- connectionHoses -->
        <dynamicallyLoadedPart node="1|0" linkNode="hoseHydraulic01In" filename="$data/shared/connectionHoses/connectors.i3d" />
        <dynamicallyLoadedPart node="1|0" linkNode="hoseHydraulic01Out" filename="$data/shared/connectionHoses/connectors.i3d" />
        <dynamicallyLoadedPart node="1|0" linkNode="hoseHydraulic02In" filename="$data/shared/connectionHoses/connectors.i3d" />
        <dynamicallyLoadedPart node="1|0" linkNode="hoseHydraulic03In" filename="$data/shared/connectionHoses/connectors.i3d" />
        <dynamicallyLoadedPart node="1|3" linkNode="hoseAirRed" filename="$data/shared/connectionHoses/connectors.i3d" />
        <dynamicallyLoadedPart node="1|2" linkNode="hoseAirYellow" filename="$data/shared/connectionHoses/connectors.i3d" />
        <dynamicallyLoadedPart node="1|1" linkNode="hoseElectric" filename="$data/shared/connectionHoses/connectors.i3d" />
    </dynamicallyLoadedParts>

    <attachable>
        <inputAttacherJoints>
            <inputAttacherJoint node="attacherJoint" jointType="trailerLow" attacherHeight="0.65" />
        </inputAttacherJoints>
        <support animationName="moveSupport"/>
        <brakeForce>1</brakeForce>
    </attachable>

    <attacherJoints>
        <attacherJoint node="semitrailerAttacherJoint" jointType="semitrailer" allowsJointLimitMovement="false" allowsLowering="false" lowerRotLimit="5 175 25" enableCollision="true" >
            <schema position="1 0" rotation="0" invertX="false" />
        </attacherJoint>
        <sounds>
            <attach template="ATTACH_01" linkNode="0>" />
        </sounds>
    </attacherJoints>

    <connectionHoses>
		<!-- in -->
        <hose inputAttacherJointIndices="1" type="hydraulicIn" node="hoseHydraulic01InRef" length="1.4" diameter="0.02" straighteningFactor="2">
           <objectChange node="hoseHydraulic01In" visibilityActive="false" visibilityInactive="true"/>
        </hose>
        <hose inputAttacherJointIndices="1" type="hydraulicOut" node="hoseHydraulic01OutRef" length="1.4" diameter="0.02" straighteningFactor="2">
            <objectChange node="hoseHydraulic01Out" visibilityActive="false" visibilityInactive="true"/>
        </hose>
        <hose inputAttacherJointIndices="1" type="hydraulicIn" node="hoseHydraulic02InRef" length="1.4" diameter="0.01" straighteningFactor="2">
           <objectChange node="hoseHydraulic02In" visibilityActive="false" visibilityInactive="true"/>
        </hose>
        <hose inputAttacherJointIndices="1" type="hydraulicIn" node="hoseHydraulic03InRef" length="1.4" diameter="0.01" straighteningFactor="2">
            <objectChange node="hoseHydraulic03In" visibilityActive="false" visibilityInactive="true"/>
        </hose>
        <hose inputAttacherJointIndices="1" type="airDoubleRed" node="hoseAirRedRef" length="1.4" diameter="0.015" straighteningFactor="1" minDeltaY="-0.410">
            <objectChange node="hoseAirRed" visibilityActive="false" visibilityInactive="true" />
        </hose>
        <hose inputAttacherJointIndices="1" type="airDoubleYellow" node="hoseAirYellowRef" length="1.4" diameter="0.015" straighteningFactor="1" minDeltaY="-0.410">
            <objectChange node="hoseAirYellow" visibilityActive="false" visibilityInactive="true" />
        </hose>
        <hose inputAttacherJointIndices="1" type="electric" node="hoseElectricRef" length="1.4" diameter="0.008" minDeltaY="-0.246">
			<objectChange node="hoseElectric" visibilityActive="false" visibilityInactive="true" />
		</hose>
		<!-- out -->
        <target attacherJointIndices="1" type="hydraulicOut" node="hydraulic01OutRef" socket="hydraulic04"/>
        <target attacherJointIndices="1" type="hydraulicIn" node="hydraulic01InRef" socket="hydraulic04"/>
        <target attacherJointIndices="1" type="hydraulicOut" node="hydraulic02OutRef" socket="hydraulic04"/>
        <!--<target attacherJointIndices="1" type="hydraulicOut" node="hydraulic03OutRef" socket="hydraulic04"/>-->
        <target attacherJointIndices="1" type="airDoubleRed" node="airRedRef" socket="air_red"/>
        <target attacherJointIndices="1" type="airDoubleYellow" node="airYellowRef" socket="air_yellow"/>
        <target attacherJointIndices="1" type="electric" node="electricRef" socket="electric"/>
    </connectionHoses>

