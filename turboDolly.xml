    <base>
        <typeDesc>$l10n_typeDesc_trailer</typeDesc>
        <filename>turboDolly.i3d</filename>
        <size width="3.5" length="5" />
        <components>
            <component centerOfMass="0 0.6 -0.8" solverIterationCount="10" mass="3500" /> <!-- original 1980 -->
        </components>
        <schemaOverlay attacherJointPosition="0 0">
            <default name="DEFAULT_IMPLEMENT"/>
            <selected name="DEFAULT_IMPLEMENT_SELECTED"/>
        </schemaOverlay>
    </base>
    <wheels>
        <wheelConfigurations>
            <wheelConfiguration name="$l10n_configuration_valueDefault" price="0" brand="LIZARD">
                <wheels>
                    <wheel filename="$data/shared/wheels/lizard/implement/385_65R22_5.xml" isLeft="true" hasTireTracks="true" hasParticles="true" >
                        <physics restLoad="0.7" repr="wheelLeft"  forcePointRatio="0.5" initialCompression="11" suspTravel="0.15" spring="120" damper="50" />
						<innerRim offset="-0.01" />
                        <wheelChock parkingNode="wheelChockLeft" />
                        <wheelChock parkingNode="wheelChockRight" isInverted="true" />
                    </wheel>
                    <wheel filename="$data/shared/wheels/lizard/implement/385_65R22_5.xml" isLeft="false" hasTireTracks="true" hasParticles="true" >
                        <physics restLoad="0.7" repr="wheelRight"  forcePointRatio="0.5" initialCompression="11" suspTravel="0.15" spring="120" damper="50" />
						<innerRim offset="-0.01" />
                    </wheel>
                </wheels>
				<objectChange node="wheelLeft" translationActive="1.05 0.56 -1.43" />
				<objectChange node="wheelRight" translationActive="-1.05 0.56 -1.43" />
				<objectChange node="wheelLeftHub" translationActive="-0.005 0 0" />
				<objectChange node="wheelRightHub" translationActive="0.005 0 0" />
            </wheelConfiguration>
            <wheelConfiguration name="$l10n_configuration_valueDefault" price="500" brand="NOKIAN">
                <wheels>
                    <wheel filename="$data/shared/wheels/nokian/TRI2/400_80R24.xml" isLeft="true" hasTireTracks="true" hasParticles="true" >
                        <physics restLoad="0.7" repr="wheelLeft"  forcePointRatio="0.5" initialCompression="11" suspTravel="0.15" spring="120" damper="50" />
						<innerRim offset="-0.01" />
                        <wheelChock parkingNode="wheelChockLeft" />
                        <wheelChock parkingNode="wheelChockRight" isInverted="true" />
                    </wheel>
                    <wheel filename="$data/shared/wheels/nokian/TRI2/400_80R24.xml" isLeft="false" hasTireTracks="true" hasParticles="true" >
                        <physics restLoad="0.7" repr="wheelRight"  forcePointRatio="0.5" initialCompression="11" suspTravel="0.15" spring="120" damper="50" />
						<innerRim offset="-0.01" />
                    </wheel>
                </wheels>
				<objectChange node="wheelLeft" translationActive="1.05 0.56 -1.43" />
				<objectChange node="wheelRight" translationActive="-1.05 0.56 -1.43" />
				<objectChange node="wheelLeftHub" translationActive="-0.005 0 0" />
				<objectChange node="wheelRightHub" translationActive="0.005 0 0" />
            </wheelConfiguration>
            <wheelConfiguration name="$l10n_configuration_valueWheelBroad" price="100" brand="MICHELIN">
                <wheels>
                    <wheel filename="$data/shared/wheels/michelin/cargoXBib/560_60R22_5.xml" isLeft="true" hasTireTracks="true" hasParticles="true" >
                        <physics restLoad="0.7" repr="wheelLeft"  forcePointRatio="0.5" initialCompression="11" suspTravel="0.15" spring="120" damper="50" />
						<innerRim offset="0.04" />
                        <wheelChock parkingNode="wheelChockLeft" />
                        <wheelChock parkingNode="wheelChockRight" isInverted="true" />
