    <storeData>
        <name>$l10n_name_turboKipper</name>
        <specs>
            <combination>$l10n_name_turboLight</combination>
        </specs>
        <functions>
            <function>$l10n_function_turboKipper</function>
            <function>$l10n_function_dolly</function>
        </functions>
        <image>store/store_turboDolly.png</image>
        <price>12900</price>
        <lifetime>600</lifetime>
        <rotation>0</rotation>
        <brand>KRAMPE</brand>
        <category>dollys</category>
